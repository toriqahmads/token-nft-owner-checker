  },
  "devDependencies": {
    "@types/jest": "^28.1.2",
    "@types/node": "^20.11.25",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.6.2",
